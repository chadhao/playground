
public class Options
{
	private int num_of_options;
	private String[] options;
	private String[] options_taken;
	
	public Options(int num)
	{
		num_of_options = num;
		options = new String[num];
		options_taken = new String[num];
		
		for (int i = 0; i < num; i++)
		{
			options[i] = "";
			options_taken[i] = "";
		}
	}
	
	
}
